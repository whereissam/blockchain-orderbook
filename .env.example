# Environment Configuration Example
# Copy this to .env and fill in your actual values

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# API keys for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key
BASESCAN_API_KEY=your_basescan_api_key

# RPC Provider URLs
VITE_ALCHEMY_API_KEY=your_alchemy_api_key_here
VITE_INFURA_API_KEY=your_infura_api_key_here

# Network Configuration
VITE_BASE_SEPOLIA_RPC=https://base-sepolia.g.alchemy.com/v2/${VITE_ALCHEMY_API_KEY}
VITE_ETHEREUM_SEPOLIA_RPC=https://ethereum-sepolia.infura.io/v3/${VITE_INFURA_API_KEY}
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
ETHEREUM_SEPOLIA_RPC_URL=https://ethereum-sepolia-rpc.publicnode.com

# Development Settings
VITE_NODE_ENV=development
VITE_DEBUG_MODE=false

# Security
VITE_ENABLE_CONSOLE_LOGS=false